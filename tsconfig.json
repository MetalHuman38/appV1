{
    "compilerOptions": {
        "target": "es6",
        "module": "ESNext",
        "moduleResolution": "node",
        "lib": ["es6", 
                "dom",
                "ESNext"
    ],
        "jsx": "react-jsx",
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,

    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}