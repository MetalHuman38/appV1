  "main": "index.js",
  "scripts": {
    "build:client": "webpack --config webpack/webpack.config.js --env env=prod",
    "build:server": "webpack --config webpack/webpack.server.config.js --mode production",
    "build": "npm run build:client && npm run build:server",
    "start:client": "webpack serve --config webpack/webpack.config.js --env env=dev",
    "start:server": "npm run build:server && nodemon dist/server.bundle.js",
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
